{
    "version": 2,
    "builds": [
        {
            "src": "api/task-webhook.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/api/task-webhook.js"
        }
    ]
} 