{
    "version": 2,
    "builds": [
        {
            "src": "api/dados-green.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/lead.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/dados-green",
            "dest": "/api/dados-green.js"
        },
        {
            "src": "/api/lead",
            "dest": "/api/lead.js"
        }
    ]
} 